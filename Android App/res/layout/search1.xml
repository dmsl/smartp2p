<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@drawable/logoback"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:weightSum="1">
    <RelativeLayout android:layout_width="match_parent" 
    				android:id="@+id/relativeLayout1" 
    				android:layout_height="match_parent">
    				
	        <ImageView android:layout_marginTop="16dp" 
	        		   android:layout_centerHorizontal="true" 
	        		   android:layout_height="250dp" 
	        		   android:layout_alignParentTop="true" 
	        		   android:layout_width="310dp" 
	        		   android:id="@+id/imageView1">
	        </ImageView>
	        <TextView android:layout_alignParentRight="true"
	        		  android:textAppearance="?android:attr/textAppearanceMedium" 
	        		  android:layout_alignBottom="@+id/seekBar1" 
	        		  android:layout_width="wrap_content" 
	        		  android:id="@+id/textView2" 
	        		  android:textColor="@android:color/white" 
	        		  android:layout_height="wrap_content" 
	        		  android:text="Recall">
	        </TextView>
	        <Button android:text="Go" 
	        		android:id="@+id/button1" 
	        		android:layout_width="90dp" 
	        		android:layout_alignParentLeft="true" 
	        		android:layout_alignParentBottom="true" 
	        		android:layout_height="wrap_content">
	        </Button>
	        <CheckBox android:layout_above="@+id/button1"
	        		  android:layout_width="wrap_content" 
	        		  android:layout_alignParentLeft="true" 
	        		  android:textColor="@android:color/white" 
	        		  android:text="Decision Making" 
	        		  android:id="@+id/checkBox1" 
	        		  android:layout_height="wrap_content">
	        </CheckBox>
	        <TextView android:textAppearance="?android:attr/textAppearanceMedium"
	        		  android:layout_width="wrap_content" 
	        		  android:id="@+id/textView1" 
	        		  android:textColor="@android:color/white" 
	        		  android:layout_height="wrap_content" 
	        		  android:text="Time" 
	        		  android:layout_below="@+id/imageView1" 
	        		  android:layout_alignParentLeft="true" 
	        		  android:layout_marginTop="34dp">
	       </TextView>
	       <TextView android:layout_width="fill_parent" 
	       			 android:id="@+id/progress" 
	       			 android:layout_height="wrap_content" 
	       			 android:layout_below="@+id/textView2" 
	       			 android:layout_alignParentLeft="true" 
	       			 android:layout_marginTop="14dp">
	       </TextView>
	       <SeekBar  android:max="100"
	       			 android:progress="50"
	       			 android:id="@+id/seekBar1" 
	       			 android:layout_height="wrap_content" 
	       			 android:layout_width="232dp" 
	       			 android:layout_alignTop="@+id/textView1" 
	       			 android:layout_toRightOf="@+id/textView1">
	      </SeekBar>
    </RelativeLayout>
</LinearLayout>